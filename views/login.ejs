<%- include('header') %>

<main>
  <div class="loginArea">
    <form action="" class="loginForm">
      <input
        class="emailInput"
        name="email"
        type="text"
        placeholder="Enter your email"
      />
      <input
        class="passwordInput"
        name="password"
        type="password"
        placeholder="Enter your password"
      />
      <!-- <input type="button" on -->
      <div class="loginButton">
        <button onclick="check(this.form)" value="Login">Login</button>
      </div>
      <a href="#" class="registerLink"
        >Don't have an account? Click here to register.</a
      >
    </form>
  </div>
</main>

<%- include('footer') %>
